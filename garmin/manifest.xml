<?xml version="1.0"?>
<iq:manifest xmlns:iq="http://www.garmin.com/xml/connectiq" version="3">
    <!-- Eversense Watchface -->
    <iq:application entry="EversenseWatchFaceApp" id="eversense-watchface" launcherIcon="@Drawables.LauncherIcon" name="@Strings.AppName" type="watchface" version="1.0.0">
        <iq:products>
            <iq:product id="vivoactive4s"/>
            <iq:product id="vivoactive4"/>
            <iq:product id="venu"/>
            <iq:product id="venu2"/>
            <iq:product id="venu2s"/>
            <iq:product id="fenix6"/>
            <iq:product id="fenix6s"/>
            <iq:product id="fenix6x"/>
            <iq:product id="forerunner245"/>
        </iq:products>
        
        <iq:permissions>
            <iq:uses-permission id="Communications"/>
            <iq:uses-permission id="Sensor"/>
        </iq:permissions>
        
        <iq:languages>
            <iq:language>eng</iq:language>
        </iq:languages>
        
        <iq:settings filename="settings.json"/>
        
        <iq:barrels/>
    </iq:application>
    
    <!-- Eversense Data Field -->
    <iq:application entry="EversenseDataFieldApp" id="eversense-datafield" launcherIcon="@Drawables.LauncherIcon" name="@Strings.DataFieldName" type="datafield" version="1.0.0">
        <iq:products>
            <iq:product id="vivoactive4s"/>
            <iq:product id="vivoactive4"/>
            <iq:product id="venu"/>
            <iq:product id="venu2"/>
            <iq:product id="venu2s"/>
            <iq:product id="fenix6"/>
            <iq:product id="fenix6s"/>
            <iq:product id="fenix6x"/>
            <iq:product id="forerunner245"/>
        </iq:products>
        
        <iq:permissions>
            <iq:uses-permission id="Communications"/>
        </iq:permissions>
        
        <iq:languages>
            <iq:language>eng</iq:language>
        </iq:languages>
        
        <iq:settings filename="datafield-settings.json"/>
        
        <iq:barrels/>
    </iq:application>
</iq:manifest>